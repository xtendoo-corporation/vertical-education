<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_education_student_form_evaluation_inherit" model="ir.ui.view">
    <field name="name">view.res.partner.form.inherit</field>
    <field name="model">res.partner</field>
    <field name="inherit_id" ref="education.view_partner_education_form"/>
    <field name="arch" type="xml">
      <group name="academic_record" position="after">
        <group string="Exam Results">
          <field name="result_ids" nolabel="1">
            <tree>
              <field name="exam_id"/>
              <field name="score"/>
            </tree>
          </field>
        </group>
      </group>
    </field>
  </record>
</odoo>
