  <odoo>
      <record id="education_timetable_line_1" model="education.timetable.line">
          <field name="course_id" ref="education.education_course_1"/>
          <field name="group_id" ref="education.education_group_1"/>
          <field name="subject_id" ref="education.education_subject_1"/>
          <field name="teacher_id" ref="education.education_teacher_1"/>
          <field name="timerange_id" ref="education_timerange_1"/>
          <field name="day" eval="0"/>
          <field name="date_from" eval="(DateTime.today() - relativedelta(months=1)).strftime('%Y-%m-%d')"/>
          <field name="date_to" eval="(DateTime.today() + relativedelta(months=1)).strftime('%Y-%m-%d')"/>
      </record>
  </odoo>
